<style>
  section {
    display: flex;
    flex-direction: column;

    padding: 55px 17px;

    background: var(--Light-Cream-2, #f0e9dd);
  }

  h2 {
    margin: 0 0 25px 0;
    color: var(--Forest-Green-2, #275130);
    text-align: center;
    font-family: Aeonik;
    font-size: 32px;
    font-style: normal;
    font-weight: 400;
  }

  p {
    max-width: 290px;
    margin: 0 auto 25px auto;

    color: var(--Forest-Green-2, #275130);
    text-align: center;
    font-family: Aeonik;
    font-size: 14px;
    font-style: normal;
    font-weight: 400;

    opacity: 0.6;
  }

  .Card {
    display: flex;
    padding: 24px 24px 28px 24px;
    flex-direction: column;
    align-items: flex-start;
    gap: 10px;
    border-radius: 16px;
    margin-bottom: 15px;

    background: var(--color-white);
  }

  .Card div {
    display: flex;
    justify-content: space-between;
    align-items: center;
    align-self: stretch;
  }

  .Card h3 {
    color: var(--Forest-Green-2, #275130);
    font-family: Aeonik;
    font-size: 24px;
    font-style: normal;
    font-weight: 400;
    letter-spacing: -0.24px;
  }

  .Card p {
    text-align: left;
    margin: 0;
    color: var(--Forest-Green-2, #275130);
    font-family: Aeonik;
    font-size: 14px;
    font-style: normal;
    font-weight: 400;
  }

  a {
    display: inline-flex;
    margin: 29px auto 0 auto;
    padding: 10px 20px 12px 20px;
    align-items: flex-start;
    gap: 10px;

    text-decoration: none;
    border-radius: 8px;
    background: var(--Forest-Green-2, #275130);

    color: var(--White, #fff);
    font-family: Aeonik;
    font-size: 14px;
    font-style: normal;
    font-weight: 400;
  }
</style>

